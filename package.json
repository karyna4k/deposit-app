  "dependencies": {
    "vue": "^3.4.29",
    "vue-router": "^4.3.3",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.66.1",
    "typescript": "~5.4.0",
    "vite": "^6.4.1",
    "vue-tsc": "^2.0.21"
  }
